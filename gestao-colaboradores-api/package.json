{
  "name": "gestao-colaboradores-api",
  "version": "1.0.0",
  "description": "API para gestão de colaboradores",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install",
    "clean": "if exist coverage (rmdir /s /q coverage) && echo '✅ Arquivos de cobertura removidos'",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs --coverage --runInBand",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:debug": "jest --config jest.config.cjs tests/debug.test.js",
    "test:integration": "jest --config jest.config.cjs tests/colaboradores.test.js",
    "test:setup": "jest --config jest.config.cjs tests/setup.test.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "superagent": "^10.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "api",
    "colaboradores",
    "gestão",
    "nodejs",
    "express"
  ],
  "author": "Daniel Eloy",
  "license": "MIT"
}
